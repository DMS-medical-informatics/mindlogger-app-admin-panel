    "moment": "^2.19.3",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "papaparse": "^4.3.7",
    "random-string": "^0.2.0",
    "re-base": "^3.2.0",
    "react": "^16.1.0",
    "react-bootstrap": "^0.31.5",
    "react-datepicker": "^1.2.1",
    "react-dom": "^16.1.0",
    "react-dropzone": "^4.2.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.0.0-beta.8",
    "react-router-redux": "^4.0.8",
